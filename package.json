{
  "name": "adept-triage-agent",
  "version": "1.11.1",
  "description": "AI-powered test failure triage GitHub Action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build dist/index.js -o dist --source-map --license licenses.txt",
    "all": "npm run build && npm run package",
    "pre-release": "./scripts/pre-release.sh",
    "verify-dist": "npm run all && git diff --exit-code dist/",
    "prepare": "husky"
  },
  "keywords": [
    "github-action",
    "test-triage",
    "ai",
    "openai",
    "test-failure-analysis"
  ],
  "author": "Adept",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.2",
    "adm-zip": "^0.5.16",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
